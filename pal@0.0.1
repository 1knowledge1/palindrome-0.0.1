#include <iostream>
#include <cstring>

using namespace std;

int main() {

    bool res = true;
    char str[30];
    int  j;

    cin.getline(str,30);
    
    j = strlen(str)-1;

    for(int i = 0; j >= 0; i++, j--)
    {
        if (str[i] == str[j]) res = true;
        else res = false;
    }

    if(res) cout << '1';
     else cout << '0';
    return 0;
}
